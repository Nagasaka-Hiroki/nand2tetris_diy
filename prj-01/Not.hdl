/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    //Nandゲートは2つの入力ピンと1つの出力ピンを持つ
    Nand( a=in , b=in, out=out);
}